<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" fx:id="pane" maxHeight="600.0" maxWidth="900.0" minHeight="600.0" minWidth="900.0" prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <AnchorPane id="Content" minHeight="0.0" minWidth="0.0" prefHeight="600.0" prefWidth="900.0" styleClass="background" stylesheets="@css/home.css">
         <children>
            <HBox layoutX="22.0" layoutY="20.0" prefHeight="550.0" prefWidth="853.0">
               <children>
                  <VBox prefHeight="550.0" prefWidth="300.0">
                     <children>
                        <Button mnemonicParsing="false" onMouseClicked="#openHomePage" prefWidth="150.0" text="Return Home">
                           <VBox.margin>
                              <Insets left="75.0" />
                           </VBox.margin>
                        </Button>
                        <ListView fx:id="devicesList" onMouseClicked="#launchVideo" prefHeight="400.0" prefWidth="280.0">
                           <VBox.margin>
                              <Insets left="10.0" right="10.0" top="50.0" />
                           </VBox.margin>
                        </ListView>
                     </children>
                  </VBox>
                  <StackPane fx:id="stack" prefHeight="550.0" prefWidth="533.0">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </StackPane>
               </children>
            </HBox>
         </children>
        </AnchorPane>
    </children>
</AnchorPane>
